<article  class="panel is-primary">
        {{#if logged_in}}
        <p class="panel-heading is-flex is-align-items-center is-justify-content-space-between">Welcome, {{golfer.name}}.<a href="/chart"><button class="button is-info">STATS</button></a></p>
        {{/if}}
        {{#if logged_in}}
        <div id="game-id" class="panel-block is-active is-size-5"><span class="panel-icon"><i class="fas fa-book"
                                aria-hidden="true"></i></span>
                <div class="played is-flex is-align-items-center is-justify-content-center mr-3">+</div>
        </div>
        {{/if}}
       
       
       {{#each golfer.games}}
        <a class="panel-block is-active is-size-5"><span class="panel-icon"><i class="fas fa-book"
                                aria-hidden="true"></i></span>
                <div id={{this.id}} class="played is-flex is-align-items-center is-justify-content-space-between">{{this.date_played}}<button
                                class="delete game-list" game-list="{{this.id}}"></button></div>
        </a>
       
       {{/each}}

        {{game.id}}

</article>

 <script src="/js/profile.js"></script>